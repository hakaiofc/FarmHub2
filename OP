--GUI
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/hakaiofc/Kavo/main/HUB"))()
local Window = Library.CreateLib("HAKAI_OFC Hub", "BloodTheme")

--MAIN
local Main Tab = Window:NewTab("Main")
